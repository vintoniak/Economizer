    <div class='box box-theme box-form'>
      <div class='box-header'>
        <h4 class='box-title'>_{SETTINGS_FORM}_</h4>
      </div>
      <div class='box-body'>
      <div class="form-group">
        <div>
          <label class="control-label col-md-4 col-sm-3">_{RESOURSE_104}_</label>
          <br>
          <br>
          <br>
        </div>
        <div class="col-md-8">
          <label class="control-label col-md-4 col-sm-3">_{LOGIN}_</label>
          <div class="col-md-8 col-sm-9">
            %LOGIN104%
          </div>
        </div>
      </div>
      <div class="form-group">
        <div class="col-md-8">
          <label class="control-label col-md-4 col-sm-3">_{PASSWORD}_</label>
          <div class="col-md-8 col-sm-9">
          %PASSWORD104%
          </div>
        </div>
      </div>
      <div class="form-group">
        <div>
          <label class="control-label col-md-4 col-sm-3">_{RESOURSE_ELECTRO}_</label>
        </div>
        <br>
        <br>
        <br>
        <div class="col-md-8">
          <label class="control-label col-md-4 col-sm-3">_{LOGIN}_</label>
          <div class="col-md-8 col-sm-9">
          %LOGIN_ELECTRO%
          </div>
        </div>
      </div>
      <div class="form-group">
        <div class="col-md-8">
          <label class="control-label col-md-4 col-sm-3">_{PASSWORD}_</label>
          <div class="col-md-8 col-sm-9">
          %PASSWORD_ELECTRO%
          </div>
        </div>
      </div>
        <div class="col-md-8 col-sm-9">
          %BUTTON_ADD%
        </div>
    </div>
  </div>
</form>

